<header id="header">
  <div id="header-outer" class="outer">
    <div id="header-inner" class="inner">
      <nav id="main-nav">
        <a id="main-nav-toggle" class="nav-icon"></a>
        <% if (theme.logo){ %>
        <a class="main-nav-link logo-link" href="/">
          <img class="main-nav-logo " src="<%- theme.logo %>" alt="logo" /> </a
        ><% } %> <% for (var i in theme.menu){ %>
        <a class="main-nav-link nav_link_flag" href="<%- url_for(theme.menu[i]) %>"
          ><%= i %></a
        >
        <% } %>
      </nav>
      <nav id="sub-nav">
        <% if (theme.rss){ %>
        <a
          id="nav-rss-link"
          class="nav-icon"
          href="<%- url_for(theme.rss) %>"
          title="<%= __('rss_feed') %>"
        ></a>
        <% } %>
        <a id="nav-search-btn" class="nav-icon" title="<%= __('search') %>"></a>
      </nav>
      <div id="search-form-wrap">
        <input type="text" class="search-input" />
        <div class="search-drop"></div>
      </div>
    </div>
  </div>
</header>
